    </Frame>

    <Frame name="UIUFO_FlyoutMenuForCatalog" inherits="UIUFO_FlyoutMenuTemplate" frameStrata="DIALOG" frameLevel="10" parent="UIParent" hidden="true">
        <Scripts>
            <OnLoad function="GLOBAL_UIUFO_FlyoutMenuForCatalog_OnLoad"/>
        </Scripts>
