<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">

<CheckButton name="UIUFO_ButtonOnFlyoutMenuTemplate" virtual="TRUE" inherits="SmallActionButtonTemplate, SecureActionButtonTemplate">
        <Scripts>
            <OnLoad function="GLOBAL_UIUFO_ButtonOnFlyoutMenu_OnLoad" />
            <OnEnter function="GLOBAL_UIUFO_ButtonOnFlyoutMenu_SetTooltip" />
            <OnLeave>
                GameTooltip:Hide()
            </OnLeave>
            <OnDragStart function="GLOBAL_UIUFO_ButtonOnFlyoutMenu_OnDragStart" />
            <OnReceiveDrag function="GLOBAL_UIUFO_ButtonOnFlyoutMenu_OnReceiveDrag" />
            <OnMouseUp function="GLOBAL_UIUFO_ButtonOnFlyoutMenu_OnMouseUp" />
            <PreClick>
                self:SetChecked(false)
            </PreClick>
        </Scripts>
        <NormalTexture/>
    </CheckButton>

